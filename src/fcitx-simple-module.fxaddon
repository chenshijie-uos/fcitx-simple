[FcitxAddon]
Name=fcitx-simple-module
Prefix=Simple
Include0="simple.h"
Function0=GetFD
Function1=GetQueue
Function2=SetEventCallback
Function3=SendEvent

[GetFD]
Name=get-fd
Return=int
CacheResult=True
ErrorReturn=-1

[GetQueue]
Name=get-queue
Return=FcitxSimpleCallQueue*
CacheResult=True

[SetEventCallback]
Name=set-event-callback
Arg0=FcitxSimpleEventHandler
Arg1=void*

[SendEvent]
Name=send-event
Arg0=FcitxSimpleEvent*
